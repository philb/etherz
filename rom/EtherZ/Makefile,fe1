# Project:   EtherZ

# Toolflags:
CCflags = -zps1 -wc -c -depend !Depend -IC: -zM 
C++flags = -c -depend !Depend -IC:
Linkflags = -rmf -c++ -o $@ 
ObjAsmflags = -NoCache -depend !Depend 
CMHGflags = 
LibFileflags = -c -o $@ 
Squeezeflags = -o $@

# Final targets:
EtherZ: o.modhdr o.etherz o.ksz8851 C:o.stubs o.call_handler
        Link $(Linkflags) o.modhdr o.etherz o.ksz8851 C:o.stubs o.call_handler
        
.c.o:;  cc $(ccflags) -o $@ $<
.s.o:;  objasm $(objasmflags) -from $< -to $@
.cmhg.o:; cmhg -o $@ $<

# Dynamic dependencies:
o.call_handler: s.call_handler
o.ksz8851:	c.ksz8851
o.ksz8851:	C:h.swis
o.ksz8851:	C:h.kernel
o.ksz8851:	h.types
